{
    "version":2,
    "builds": [
        {
            "src": "finalblog/finalblog/wsgi.py",
            "use": "@vercel/python",
            "config": {"maxLambdaSize": "15mb", "runtime": "python 3.10"}
        },
        {
            "src": "build_files.sh",
            "use": "@vercel/static-build",
            "config": {
                "distDir":"staticfiles_build"
            }
        }
    ],
    "routes": [
        {
        "src": "/(.*)",
        "dest": "/static/$1"
        },
        {
            "src": "/(.*)",
            "dest": "poll/wsgi.py"
        }
    ]
}